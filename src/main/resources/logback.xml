<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%red(%d{[YYYY-MM-dd HH-mm-ss]}) %red([INFORMATION]) %red(%msg%n)</pattern>
        </encoder>
    </appender>

    <root level="info">
    </root>
</configuration>
